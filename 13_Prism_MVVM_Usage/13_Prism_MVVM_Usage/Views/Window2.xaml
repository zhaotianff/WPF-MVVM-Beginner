<Window x:Class="_12_Prism_MVVM_Usage.Views.Window2"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:_12_Prism_MVVM_Usage.Views"
        xmlns:prism="http://prismlibrary.com/"
        prism:ViewModelLocator.AutoWireViewModel="True"
        mc:Ignorable="d"
        Title="Window2" Height="450" Width="400">
    <StackPanel>
        <TextBlock Margin="5" Text="{Binding Id}"></TextBlock>
        <TextBlock Text="{Binding Name}"></TextBlock>
        <TextBlock Text="{Binding Score}"></TextBlock>
    </StackPanel>
</Window>
